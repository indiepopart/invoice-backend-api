{
  "version": 2,
  "functions":{
    "index.js": {
      "runtime": "@now/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS,POST",
        "Access-Control-Allow-Headers": "Content-Type,x-access-token"
      }
    }
  ]
}
